{
    "name": "gerardwalace/flarum-react-zoom-pan-pitch",
    "description": "Use React to create a zoomable, pannable, pitchable image viewer",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2.0"
    },
    "authors": [
        {
            "name": "Gerard Walace",
            "email": "gerard.walace@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "GerardWalace\\FlarumReactZoomPanPitch\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "ReactZoomPanPitch",
            "category": "",
            "icon": {
                "name": "fas fa-search-plus",
                "color": "#fff",
                "backgroundColor": "#684ba6"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": false,
                "forum": true,
                "js": true,
                "jsCommon": false,
                "css": false,
                "locale": false,
                "gitConf": true,
                "githubActions": false,
                "prettier": true,
                "typescript": false,
                "bundlewatch": false,
                "backendTesting": false,
                "editorConfig": false,
                "styleci": false
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
